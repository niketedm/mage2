<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <frequentlybought>
            <general>
                <enabled>1</enabled>
                <product_method>fbt</product_method>
                <block_name>Frequently bought together</block_name>
                <item_limit>3</item_limit>
                <enable_add_to_wishlist>1</enable_add_to_wishlist>
                <remove_related_block>1</remove_related_block>
                <use_popup>0</use_popup>
            </general>
        </frequentlybought>
    </default>
</config>
