        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="checkout.digitalbuy.modal-auth-complete" template="Synchrony_DigitalBuy::digitalbuy/revolving/modal/auth/complete.phtml" cacheable="false">
